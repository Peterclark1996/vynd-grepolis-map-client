#! /app/bin/node

const https = require('https')
const options = {
    hostname: 'https://grepolis-live-map.herokuapp.com',
    port: 443,
    path: '/updateWorldData',
    method: 'GET'
}

const req = https.request(options, res => {
    console.log(`statusCode: ${res.statusCode}`)

    res.on('data', d => {
        process.stdout.write(d)
    })
})

req.on('error', error => {
    console.error(error)
})

req.end()

process.exit()